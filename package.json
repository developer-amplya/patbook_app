    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "displayName": "patbook",
    "cordova": {
        "platforms": [
            "android",
            "ios"
        ],
        "plugins": {
            "cordova-plugin-statusbar": {},
            "cordova-plugin-battery-status": {},
            "cordova-plugin-camera": {},
